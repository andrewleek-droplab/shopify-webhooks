{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"rootDir": "src",
		"outDir": "dist",
		"sourceMap": true,
		"strict": true,
		"lib": ["dom", "esnext"],
		"esModuleInterop": true,
		"declaration": true,
		"declarationMap": true
	},
	"exclude": ["node_modules", "dist"]
}